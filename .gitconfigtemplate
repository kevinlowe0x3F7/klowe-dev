# This is Git's per-user configuration file.
# Remove user info from template
[init]
	defaultBranch = develop

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true    # use n and N to move between diff sections
    side-by-side = true
    dark = true

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
